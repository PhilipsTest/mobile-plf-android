
public class AcadGildTutorialTwo {
	
	
	public static void main(String[] args)
	{
		
		int x = 3;
		
		if(x == 2)
		{
		  System.out.println(" Hello AcadGild");
		}else
		{
			System.out.println("I am in else statement boss!! because the boolean statement returned false");
		}
		
		System.out.println("AcadGild trianing is nice!!");
		
		
	}// End Brace of static void main mehtod
	
}// End brace of AcadGildTutorialTwo
