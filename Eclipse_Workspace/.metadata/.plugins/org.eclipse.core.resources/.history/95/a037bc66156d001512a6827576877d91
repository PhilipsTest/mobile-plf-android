
public class AcadGildPractice {

	public static void main(String[] acadgild) {

		System.out.println("Starting Portion");
		int i = 3; 
		
		do
		{
			System.out.println(" Inside the Looping with i value : " + i);
		}while(i > 5);
		
		System.out.println("End Portion");
		

	} // This is the end brace of the staic void main method.

} // This is the end brace of the Class
