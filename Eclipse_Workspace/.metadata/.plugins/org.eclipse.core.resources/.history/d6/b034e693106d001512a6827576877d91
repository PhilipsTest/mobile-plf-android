
public class AcadGildPractice {

	public static void main(String[] acadgild) {

		System.out.println("Hello AcadGild");

		int month = 1;
		
		switch(month)
		{
		  case 1 :
			  System.out.println("I am Number ONe");
		  case 2:
			  System.out.println("I am Number two");
			  break;
		  case 3 :
			  System.out.println("I am Number Three");
		}
	}
}
