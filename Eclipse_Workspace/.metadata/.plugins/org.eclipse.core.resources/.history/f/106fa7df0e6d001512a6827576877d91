
public class AcadGildPractice {

	public static void main(String[] acadgild) {

		System.out.println("Hello AcadGild");

		byte value = 3;

		if (value == 1)
		{
			System.out.println("Yes Boss, Value is 1");
		} else if(value == 2)
		{
			System.out.println("Yes Boss, Value is 2");
		}else if(value == 3)
		{
			System.out.println("Yes Boss, Value is 3");
		}
		else if(value == 10)
		{
			System.out.println("Yes Boss, Value is 10");
		}else
		{
			System.out.println("I am in else Block boss, I don't know any values");
		}

	}

}
