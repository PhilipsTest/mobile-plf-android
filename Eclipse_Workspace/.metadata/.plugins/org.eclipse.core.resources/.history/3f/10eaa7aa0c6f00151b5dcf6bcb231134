
public class SampleOne {
	
   	public static void main (String[]  args)
   	{
   		byte mByteValue = 3;
   		
   		float mFloatValue = 12.3f;
   		
   		int mSum = (int)mFloatValue;
   		
   		int sum, minus , plus;
   		
   		int sum;
   		int minus;
   		int plus;

   		System.out.println("Printing the value here : "+ mSum);

   	}//static void main method.
