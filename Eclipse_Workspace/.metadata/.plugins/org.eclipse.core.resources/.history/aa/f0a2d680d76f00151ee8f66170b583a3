
public class AcadGildTutorialTwo {
	public static void main(String[] args)
	{
		for(int x = 1 ;  x <= 5  ; x++ ) 
		{
			System.out.println("Printing the x value : "+ x);
			
			if( x == 4)
			{
				break;
			}
				
		}
		
	}// End Brace of static void main mehtod
	
}// End brace of AcadGildTutorialTwo


