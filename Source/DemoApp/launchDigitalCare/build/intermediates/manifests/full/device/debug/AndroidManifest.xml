    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/app_theme_vertical" >
        <activity
            android:name="com.philips.cdp.sampledigitalcare.DummyScreen"
            android:configChanges="orientation|screenSize|locale|layoutDirection" >
            <intent-filter>
                <action android:name="android.intent.action.dummyScreen" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.philips.cdp.sampledigitalcare.SampleActivity"
            android:configChanges="orientation|screenSize|locale|layoutDirection"
            android:theme="@style/app_theme_vertical" >
        </activity>
        <activity
            android:name="com.philips.cdp.sampledigitalcare.LaunchDigitalCare"
            android:configChanges="orientation|screenSize|locale|layoutDirection"
            android:label="@string/app_name"
            android:theme="@style/app_theme_vertical" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Credentials For LocatePhilips Near you feature(Google Map) -->
