<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.philips.cdp.sampledigitalcare"
    android:versionCode="141"
    android:versionName="5.3.0.3" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />
    <!-- <uses-sdk tools:overrideLibrary="com.philips.cdp.digitalcare"/> -->


    <!-- Credentials For LocatePhilips Near you feature(Google Map) -->
    <uses-permission android:name="com.philips.cdp.sampledigitalcare.permission.MAPS_RECEIVE" />

    <permission
        android:name="com.philips.cdp.sampledigitalcare.permission.MAPS_RECEIVE"
