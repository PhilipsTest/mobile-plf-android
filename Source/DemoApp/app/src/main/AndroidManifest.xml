<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.philips.platform.appinfra.demo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".AppInfraApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


            <activity
            android:name="com.philips.platform.appinfra.demo.AppInfraMainActivity"
            android:label="@string/title_activity_app_infra_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>
            <activity
                android:name="com.philips.platform.appinfra.demo.SecureStorageActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
            </activity>
            <activity
                android:name="com.philips.platform.appinfra.demo.LoggingActivity"
                android:label="Log Test"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.NoActionBar">


        </activity>

        <activity
            android:name="com.philips.platform.appinfra.demo.PrxLauncherActivity"
            android:label="Log Test"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">


        </activity>

        <activity
            android:name="com.philips.platform.appinfra.demo.AIATDemoPage"
            android:label="Log Test"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">


        </activity>

        <activity
            android:name="com.philips.platform.appinfra.demo.LocalMatchActivity"
            android:label="Log Test"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">


        </activity>
    </application>

</manifest>
