<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="multiprod_actionbarIconArrowImageView" parent="multiprod_actionbar_back_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="multiprod_actionbarFontTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/actionbar_height</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/multiprod_actionbar_title_color</item>
        <item name="android:text">@string/actionbar_title_support</item>
        <item name="android:textSize">@dimen/title_text_size_medium</item>
    </style>
</resources>