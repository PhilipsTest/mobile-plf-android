<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="UIDProgressBarCircularDeterminateBig" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:layout_width">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:layout_height">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:minWidth">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:minHeight">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:indeterminate">false</item>
        <item name="uidIsLinearProgressBar">false</item>
        <item name="android:progressDrawable">@drawable/uid_circular_progress_bar_big</item>
        <item name="uidProgressBarCircularEndColor">?attr/uidTrackDetailNormalOnBackgroundColor
        </item>
    </style>

    <style name="UIDProgressBarCircularIndeterminateBig" parent="UIDProgressBarCircularDeterminateBig">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDuration">@integer/uid_progress_bar_circular_big_duration
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/uid_circular_progress_bar_big_indeterminate
        </item>
    </style>
</resources>