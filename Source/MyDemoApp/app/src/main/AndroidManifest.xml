<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="cdp.philips.com.mydemoapp">

    <application
        android:name="cdp.philips.com.mydemoapp.DataSyncApplicationClass"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name_data_sync"
        android:supportsRtl="true"
        android:theme="@style/Theme.Philips.DarkBlue.WhiteBackground"
        tools:replace="android:theme,android:label">

        <activity android:name=".activity.DemoActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>