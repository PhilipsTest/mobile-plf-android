<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PhilipsUIKit">
        <attr name="actionBarBackground" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="ActionButton">
        <attr name="actionButtonShape" format="enum">
            <enum name="square" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
        <attr name="actionButtonBgColor" format="color"/>
        <attr name="actionButtonBgColorPressed" format="color"/>
        <attr name="actionButtonImageDrawable" format="reference"/>
        <attr name="actionButtonStyleRef" format="reference"/>
        <attr name="actionButtonShadow" format="boolean"/>
    </declare-styleable>

    <attr name="splashTitleStyle" format="reference" />

    <declare-styleable name="AppCompactImageView">
        <attr name="vectorsrc" format="reference"/>
    </declare-styleable>
</resources>
