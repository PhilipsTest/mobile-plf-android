<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PhilipsUIKit">
        <attr name="actionBarBackground" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="MiscButton">
        <attr name="miscButtonShape" format="enum">
            <enum name="square" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
        <attr name="miscButtonBgColor" format="color"/>
        <attr name="miscButtonBgColorPressed" format="color"/>
        <attr name="miscButtonImageDrawable" format="reference"/>
        <attr name="miscButtonStyle" format="reference"/>
    </declare-styleable>
</resources>
