<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="bottom"
    android:paddingTop="33dp"
    android:paddingLeft="33dp"
    android:paddingRight="33dp"
    android:background="?attr/gradBackground"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:text="Mobile app"
        android:layout_gravity="center"
        android:textColor="@color/uikit_white"
        fontPath="fonts/centralesanslight.ttf"
        android:layout_marginBottom="30dp"
        android:textSize="33dp"/>

    <LinearLayout
        android:id="@+id/row_layout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.25"
        android:layout_marginBottom="-1.5dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"
            android:clickable="true"
            android:orientation="vertical"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:background="?attr/springboardgradBackground"
            android:paddingBottom="15dp"
            android:gravity="center_horizontal|center_vertical|bottom">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_gravity="center"
                android:foregroundGravity="center_vertical|center_horizontal|center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/image"
                app:themeStyle="theme" />

            <TextView
                android:id="@+id/textView1"
                fontPath="fonts/centralesansbook.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:text="TextView"
                android:textColor="@color/uikit_white"
                android:textSize="16sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"
            android:background="?attr/springboardgradBackground"
            android:clickable="true"
            android:layout_marginBottom="20dp"
            android:gravity="center_horizontal|center_vertical|bottom"
            android:orientation="vertical"
            android:paddingBottom="15dp"
            >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/image"
                app:themeStyle="theme" />

            <TextView
                android:id="@+id/textView2"
                fontPath="fonts/centralesansbook.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:text="TextView"
                android:textColor="@color/uikit_white"
                android:textSize="16sp" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row_layout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.25"
        android:layout_marginBottom="20dp"

        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"
            android:background="?attr/springboardgradBackground"
            android:paddingBottom="15dp"
            android:clickable="true"
            android:orientation="vertical"
            android:layout_marginRight="20dp"
            android:gravity="center_horizontal|center_vertical|bottom">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/image"
                app:themeStyle="theme" />

            <TextView
                android:id="@+id/textView3"
                fontPath="fonts/centralesansbook.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:text="TextView"
                android:textColor="@color/uikit_white"
                android:textSize="16sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"
            android:background="?attr/springboardgradBackground"
            android:clickable="true"
            android:orientation="vertical"
            android:paddingBottom="15dp"
            android:gravity="center_horizontal|center_vertical|bottom">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/image"
                app:themeStyle="theme" />

            <TextView
                android:id="@+id/textView4"
                fontPath="fonts/centralesansbook.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:text="TextView"
                android:textColor="@color/uikit_white"
                android:textSize="16sp" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row_layout3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.25"
        android:layout_marginBottom="20dp"

        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:background="?attr/springboardgradBackground"
            android:layout_marginRight="15dp"
            android:layout_weight="0.5"
            android:clickable="true"
            android:orientation="vertical"
            android:paddingBottom="15dp"
            android:gravity="center_horizontal|center_vertical|bottom">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/image"
                app:themeStyle="theme" />

            <TextView
                android:id="@+id/textView5"
                fontPath="fonts/centralesansbook.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/uikit_white"
                android:textSize="16sp" />

        </LinearLayout>



        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"
            android:background="?attr/springboardgradBackground"
            android:clickable="true"
            android:orientation="vertical"
            android:paddingBottom="15dp"
            android:gravity="center_horizontal|center_vertical|bottom">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/image"
                app:themeStyle="theme" />

            <TextView
                android:id="@+id/textView6"
                fontPath="fonts/centralesansbook.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/uikit_white"
                android:textSize="16sp" />

        </LinearLayout>
    </LinearLayout>



</LinearLayout><!--160 , 180-->
