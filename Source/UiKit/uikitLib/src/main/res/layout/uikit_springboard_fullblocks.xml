                android:layout_marginTop="10dp"
                android:gravity="center"
                android:text="@string/uikit_springboard_textview_label"
                android:duplicateParentState="true"

                android:textColor="?attr/uikit_springBoardTextColor"
                android:textSize="16sp" />

        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/HorizonView3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="?attr/uikit_springBoardDivider" />

    <LinearLayout
