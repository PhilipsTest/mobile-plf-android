        <item>@string/product_info</item>
        <item>@string/FAQ_KEY</item>
        <item>@string/contact_us</item>
        <item>@string/dcc_tellUs_header</item>
        <item>@string/Change_Selected_Product</item>
        <item>@string/RA_Product_Registration_Text</item>

    </array>

    <!--
