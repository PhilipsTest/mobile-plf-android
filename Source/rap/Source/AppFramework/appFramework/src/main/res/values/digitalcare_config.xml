        <item>@string/FAQ_KEY</item>
        <item>@string/dcc_productDownloadManual</item>
        <item>@string/dcc_productInformationOnWebsite</item>
        <item>@string/dcc_productDownloadDfu</item>
    </string-array>

    <!-- List of social service providers localised resource keys, please follow naming conventions -->
    <string-array name="social_service_provider_menu_title">
        <item>@string/dcc_twitter</item>
        <item>@string/dcc_facebook</item>
    </string-array>

    <!-- List of social service providers image names, please follow naming conventions -->
    <string-array name="social_service_provider_menu_resources">
        <item>@string/dls_socialmediatwitter</item>
        <item>@string/dls_socialmediafacebook</item>
    </string-array>

    <!-- Do you want Live Chat Option -->
    <bool name="live_chat_required">true</bool>
    <string name="consumerCareChat_desc">@string/dcc_chat_desc</string>
</resources>