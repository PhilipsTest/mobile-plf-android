    -->
    <array name="main_menu_resources">
        <item>@string/dls_information</item>
        <item>@string/dls_questionmark</item>
        <item>@string/dls_balloonspeech</item>
        <item>@string/dls_conversationspeech</item>
        <item>@string/dls_revise</item>
        <item>@string/dls_shield</item>
    </array>

    <!-- List of product menu localised resource keys, please follow naming conventions -->
