<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/action_bar_pth"
        layout="@layout/pth_provider_details_d_list" />
    <View android:background="@color/uid_gray_level_5"
        android:layout_below="@+id/action_bar_pth"
        android:id="@+id/horizontalLine"
        android:layout_width = "match_parent"
        android:layout_height="1dp"
        />
    <RelativeLayout
        android:id="@+id/visitCostLayout"
        android:layout_below="@+id/horizontalLine"
        android:layout_marginStart="@dimen/uid_grid_title_padding"
        android:layout_marginEnd="@dimen/uid_grid_title_padding"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.philips.platform.uid.view.widget.Label
            android:id="@+id/visitCostLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Visit cost"
            android:textSize="@dimen/uid_grid_descrption_size"
            android:textColor="?attr/uidLabelValueNormalTextColor"
            />
        <com.philips.platform.uid.view.widget.Label
            android:layout_below="@+id/visitCostLabel"
            android:id="@+id/priceBeforeLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Price before insurance"
            android:textSize="@dimen/uid_grid_descrption_size"
            android:textColor="?attr/uidLabelValueNormalTextColor"
            />
        <com.philips.platform.uid.view.widget.Label
            android:id="@+id/visitCostValueLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$59"
            android:textSize="@dimen/uid_grid_descrption_size"
            android:textColor="?attr/uidLabelValueNormalTextColor"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="22dp" />
    </RelativeLayout>
    <View android:background="@color/uid_gray_level_5"
        android:layout_below="@+id/visitCostLayout"
        android:id="@+id/horizontalLineOne"
        android:layout_width = "match_parent"
        android:layout_height="1dp"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/horizontalLineOne"
        android:id="@+id/profileLabel"
        android:background="@color/uid_gray_level_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/profileLabel"
        android:id="@+id/ratingLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rating"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <com.philips.platform.uid.view.widget.RatingBar
        android:layout_below="@+id/ratingLabel"
        android:id="@+id/providerRatingValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rating="3.6"
        android:stepSize=".1"
        android:layout_marginStart="@dimen/uid_grid_title_padding"
        android:layout_marginEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        style="@style/UIDRatingBarStandardDisplay"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <View android:background="@color/uid_gray_level_5"
        android:layout_below="@+id/providerRatingValue"
        android:id="@+id/horizontalLineTwo"
        android:layout_width = "match_parent"
        android:layout_height="1dp"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/horizontalLineTwo"
        android:id="@+id/spokenLanguageLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Spoken Language"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/spokenLanguageLabel"
        android:id="@+id/spokenLanguageValueLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="English, Spanish"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <View android:background="@color/uid_gray_level_5"
        android:layout_below="@+id/spokenLanguageValueLabel"
        android:id="@+id/horizontalLineThree"
        android:layout_width = "match_parent"
        android:layout_height="1dp"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/horizontalLineThree"
        android:id="@+id/yearsOfExpLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Years of experience"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/yearsOfExpLabel"
        android:id="@+id/yearsOfExpValueLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="17"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <View android:background="@color/uid_gray_level_5"
        android:layout_below="@+id/yearsOfExpValueLabel"
        android:id="@+id/horizontalLineFour"
        android:layout_width = "match_parent"
        android:layout_height="1dp"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/horizontalLineFour"
        android:id="@+id/graduatedLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Graduated"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/graduatedLabel"
        android:id="@+id/graduatedValueLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Harvard Medical in 1992"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <View android:background="@color/uid_gray_level_5"
        android:layout_below="@+id/graduatedValueLabel"
        android:id="@+id/horizontalLineFive"
        android:layout_width = "match_parent"
        android:layout_height="1dp"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/horizontalLineFive"
        android:id="@+id/aboutMeLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="About Me"
        android:background="@color/uid_gray_level_5"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <com.philips.platform.uid.view.widget.Label
        android:layout_below="@+id/aboutMeLabel"
        android:id="@+id/aboutMeValueLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="When the user takes the DOD  ow, this doc- tor detail is shown after the system matches a doctor. The user can read the whole pro le and continue to the waiting screen"
        android:paddingStart="@dimen/uid_grid_title_padding"
        android:paddingEnd="@dimen/uid_grid_title_padding"
        android:textSize="@dimen/uid_grid_descrption_size"
        android:textColor="?attr/uidLabelValueNormalTextColor"
        />
    <View android:background="@color/uid_gray_level_5"
        android:layout_below="@+id/aboutMeValueLabel"
        android:id="@+id/horizontalLineSix"
        android:layout_width = "match_parent"
        android:layout_height="1dp"
        />
</RelativeLayout>