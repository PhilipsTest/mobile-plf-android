<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.philips.cl.di.dev.pa"
    android:versionCode="139"
    android:versionName="4.2.0.4">

    <uses-sdk android:minSdkVersion="10"
        android:targetSdkVersion="23"/>
   <!-- <uses-sdk tools:overrideLibrary="com.philips.cdp.digitalcare"/>  -->

    <!--Credentials For LocatePhilips Near you feature(Google Map)-->
    <uses-permission android:name="com.philips.cl.di.dev.pa.permission.MAPS_RECEIVE" />
    <permission
        android:name="com.philips.cl.di.dev.pa.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/app_theme_vertical"
        tools:replace="android:icon,android:label,android:theme">
        <activity
            android:name="com.philips.cdp.sampledigitalcare.DummyScreen"
            android:configChanges="orientation|screenSize|locale|layoutDirection">
            <intent-filter>
                <action android:name="android.intent.action.dummyScreen" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.philips.cdp.sampledigitalcare.launcher.componentLaunch.SampleActivity"
            android:configChanges="orientation|screenSize|locale|layoutDirection"
            android:theme="@style/app_theme_vertical">
        </activity>

        <activity
            android:name="com.philips.cdp.sampledigitalcare.launcher.uAppComponetLaunch.MicroAppFragmentActivity"
            android:configChanges="orientation|screenSize|locale|layoutDirection"
            android:theme="@style/app_theme_vertical"></activity>
        <activity
            android:name="com.philips.cdp.sampledigitalcare.launcher.uAppComponetLaunch.MicroAppLauncher"
            android:configChanges="orientation|screenSize|locale|layoutDirection"
            android:label="@string/app_name"
            android:theme="@style/app_theme_vertical">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--Credentials For LocatePhilips Near you feature(Google Map)-->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!-- Naveen : AIzaSyCMHfSC_TqL_PNqJX7pbaIyl154VnFrhec-->
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyABLxPKwgjrCBnnLvP55yAYnhpAHZmhLvY" />
    </application>

</manifest>