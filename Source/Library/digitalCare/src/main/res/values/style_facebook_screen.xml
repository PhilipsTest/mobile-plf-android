    <style name="fbMainHeader">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/button_background</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:textSize">@dimen/title_text_size_large</item>
    </style>

    <style name="fbFromMainHeader">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:textColor">#252525</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:textSize">@dimen/title_text_size_medium</item>
    </style>


    <style name="fboptouttext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">#252525</item>
        <item name="android:textSize">@dimen/title_text_size_small</item>
    </style>

