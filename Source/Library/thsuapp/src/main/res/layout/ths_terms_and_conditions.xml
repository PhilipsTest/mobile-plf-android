<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nop_container"
    android:layout_width="match_parent"
    android:background="?attr/uidContentPrimaryBackgroundColor"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:paddingTop="@dimen/ths_fab_margin"
        android:layout_height="match_parent">
        
        <WebView
            android:paddingLeft="@dimen/ths_fab_margin"
            android:paddingRight="@dimen/ths_fab_margin"
            android:id="@+id/ths_webview"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </WebView>
    </ScrollView>

</RelativeLayout>