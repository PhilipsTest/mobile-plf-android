<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:reglib="http://reg.lib/schema"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/reg_text_padding_bottom"
    android:orientation="horizontal">

    <com.philips.cdp.registration.ui.customviews.XTextView
        android:id="@+id/tv_bullet_ic_text"
        style="@style/reg_password_hint_text"
        android:layout_marginBottom="0dip"
        android:layout_width="wrap_content"
        android:text="@string/reg_Create_Account_PasswordHint_Length_lbltxt"
        reglib:fontAssetName="CentraleSans-Book.OTF" />


    <com.philips.cdp.registration.ui.customviews.XTextView
        android:id="@+id/tv_bullet_text"
        style="@style/reg_password_hint_text"
        android:layout_width="match_parent"
        android:layout_marginBottom="0dip"
        android:text="@string/reg_Create_Account_PasswordHint_Length_lbltxt"
        reglib:fontAssetName="CentraleSans-Book.OTF" />


</LinearLayout>