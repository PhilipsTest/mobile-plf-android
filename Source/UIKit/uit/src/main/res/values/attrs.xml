<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Philipsuit">
        <attr name="uitContentPrimary" format="reference|color"/>
        <attr name="uitContentSecondary" format="reference|color"/>
        <attr name="uitContentTextColor" format="reference|color"/>

        <attr name="uitNavigationPrimaryColor" format="reference|color"/>
        <attr name="uitNavigationSecondaryColor" format="reference|color"/>
        <attr name="uitNavigationTextColor" format="reference|color"/>

        <attr name="uitControlSecondaryColor" format="reference|color"/>
        <attr name="uitControlSecondaryTextColor" format="reference|color"/>
        <attr name="uitControlSecondaryEnabledColor" format="reference|color"/>
        <attr name="uitControlSecondaryPressedColor" format="reference|color"/>
        <attr name="uitControlSecondaryDisabledAlpha" format="reference|float"/>
        <attr name="uitControlSecondaryTextDisabledAlpha" format="reference|float"/>

        <attr name="uitControlPrimaryEnabledColor" format="reference|color"/>
        <attr name="uitControlPrimaryPressedColor" format="reference|color"/>
        <attr name="uitControlPrimaryTextColor" format="reference|color"/>
        <attr name="uitControlPrimaryDisabledAlpha" format="reference|float"/>
        <attr name="uitControlPrimaryTextDisabledAlpha" format="reference|float"/>

        <attr name="uitControlPrimaryAlternateEnabledColor" format="reference|color"/>
        <attr name="uitControlPrimaryAlternatePressedColor" format="reference|color"/>
        <attr name="uitControlPrimaryAlternateTextColor" format="reference|color"/>
        <attr name="uitControlPrimaryAltDisabledAlpha" format="reference|float"/>
        <attr name="uitControlPrimaryAltTextDisabledAlpha" format="reference|float"/>
        <attr name="uitApplicationBackground" format="reference|color"/>
    </declare-styleable>


    <declare-styleable name="DlsColorPallet">
        <attr name="uitColorLevel90" format="reference|color"/>
        <attr name="uitColorLevel85" format="reference|color"/>
        <attr name="uitColorLevel80" format="reference|color"/>
        <attr name="uitColorLevel75" format="reference|color"/>
        <attr name="uitColorLevel70" format="reference|color"/>
        <attr name="uitColorLevel65" format="reference|color"/>
        <attr name="uitColorLevel60" format="reference|color"/>
        <attr name="uitColorLevel55" format="reference|color"/>
        <attr name="uitColorLevel50" format="reference|color"/>
        <attr name="uitColorLevel45" format="reference|color"/>
        <attr name="uitColorLevel40" format="reference|color"/>
        <attr name="uitColorLevel35" format="reference|color"/>
        <attr name="uitColorLevel30" format="reference|color"/>
        <attr name="uitColorLevel25" format="reference|color"/>
        <attr name="uitColorLevel20" format="reference|color"/>
        <attr name="uitColorLevel15" format="reference|color"/>
        <attr name="uitColorLevel10" format="reference|color"/>
        <attr name="uitColorLevel05" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="UitComponentType">
        <attr name="uitComponentType" format="enum">
            <enum name="Primary" value="0"/>
            <enum name="AltPrimary" value="1"/>
            <enum name="Secondary" value="2"/>
            <enum name="Quiet" value="2"/>
            <enum name="Circular" value="2"/>
            <enum name="Accent" value="3"/>
        </attr>
    </declare-styleable>

</resources>
