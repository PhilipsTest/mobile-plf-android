<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="UITNavigationBar" parent="Theme.AppCompat.Light.NoActionBar" tools:ignore="NewApi">
        <item name="android:background">?attr/uitActionBarBackground</item>
        <item name="background">?attr/uitActionBarBackground</item>
        <item name="android:textColorPrimary">?attr/uitControlPrimaryTextColor</item>


    </style>

    <style name="UITNavigationbarUltraLight">
        <item name="uitActionBarBackground">@color/uitColorWhite</item>
    </style>

    <style name="UITNavigationbarVeryLight">
        <item name="uitActionBarBackground">?attr/uitColorLevel05</item>
    </style>

    <style name="UITNavigationbarLight">
        <item name="uitActionBarBackground">?attr/uitColorLevel20</item>
    </style>

    <style name="UITNavigationbarBright">
        <item name="uitActionBarBackground">?attr/uitColorLevel40</item>
    </style>

    <style name="UITNavigationbarVeryDark">
        <item name="uitActionBarBackground">?attr/uitColorLevel65</item>
    </style>
</resources>