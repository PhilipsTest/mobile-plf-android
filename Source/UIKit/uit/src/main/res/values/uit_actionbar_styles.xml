<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="UITNavigationBar" parent="Theme.AppCompat.Light.NoActionBar" tools:ignore="NewApi">
        <item name="android:background">?attr/uidActionBarBackground</item>
        <item name="background">?attr/uidActionBarBackground</item>
        <item name="android:textColorPrimary">?attr/uidControlPrimaryTextColor</item>

    </style>

    <style name="UITNavigationbarUltraLight">
        <item name="uidActionBarBackground">@color/uidColorWhite</item>
    </style>

    <style name="UITNavigationbarVeryLight">
        <item name="uidActionBarBackground">?attr/uidColorLevel10</item>
    </style>

    <style name="UITNavigationbarLight">
        <item name="uidActionBarBackground">?attr/uidColorLevel25</item>
    </style>

    <style name="UITNavigationbarBright">
        <item name="uidActionBarBackground">?attr/uidColorLevel40</item>
    </style>

    <style name="UITNavigationbarVeryDark">
        <item name="uidActionBarBackground">?attr/uidColorLevel70</item>
    </style>
</resources>