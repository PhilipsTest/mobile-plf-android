<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape
            android:thicknessRatio="@dimen/uid_progress_bar_circular_big_thicknessRatio"
            android:innerRadiusRatio="@dimen/uid_progress_bar_circular_big_innerRadiusRatio"
            android:shape="ring"
            android:useLevel="false">
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="0"
            android:toDegrees="360">
            <shape
                android:thicknessRatio="@dimen/uid_progress_bar_circular_big_thicknessRatio"
                android:innerRadiusRatio="@dimen/uid_progress_bar_circular_big_innerRadiusRatio"
                android:shape="ring"
                android:useLevel="false">
            </shape>
        </rotate>
    </item>
</layer-list>