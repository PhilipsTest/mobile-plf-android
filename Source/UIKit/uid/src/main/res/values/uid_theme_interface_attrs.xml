<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UIDButton">

        <attr name="uidButtonBackgroundColorList" format="reference"/>
        <attr name="uidButtonTextColorList" format="reference"/>
        <attr name="uidButtonDrawableColorList" format="reference"/>
        <attr name="uidButtonImageColorList" format="reference"/>
        <attr name="uidButtonImageDrawableHeight" format="dimension"/>
        <attr name="uidButtonImageDrawableWidth" format="dimension"/>
        <attr name="uidButtonImageDrawableSrc" format="reference"/>
        <attr name="uidButtonCenter" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="UIDImageButton">
        <attr name="uidImageButtonDrawableHeight" format="dimension"/>
        <attr name="uidImageButtonDrawableWidth" format="dimension"/>
        <attr name="uidImageButtonDrawableSrc" format="reference"/>
        <attr name="uidImageButtonDrawableColorList" format="reference"/>
        <attr name="uidImageButtonColorList" format="reference"/>
    </declare-styleable>

    <declare-styleable name="UIDTextEditBox">
        <attr name="uidInputTextFillColorList" format="reference"/>
        <attr name="uidInputTextFillBackground" format="reference"/>
        <attr name="uidInputTextFillBackgroundColorList" format="reference"/>
        <attr name="uidInputTextBorderWidth" format="reference|dimension"/>
        <attr name="uidInputTextBorderBackground" format="reference"/>
        <attr name="uidInputTextBorderBackgroundColorList" format="reference"/>
        <attr name="uidInputTextTextSelector" format="reference"/>
        <attr name="uidInputTextHintTextSelector" format="reference"/>
        <attr name="uidInputTextWithClearButton" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="UIDNavigationBar">

        <attr name="uidNavigationShadowLevelOne" format="reference|color"/>
        <attr name="uidNavigationShadowLevelOneAlpha" format="reference|float"/>
        <attr name="uidNavigationShadowLevelTwo" format="reference|color"/>
        <attr name="uidNavigationShadowLevelTwoAlpha" format="reference|float"/>
    </declare-styleable>

    <declare-styleable name="UIDProgressBar">
        <attr name="uidIsLinearProgressBar" format="boolean"/>
        <attr name="uidProgressBarCircularEndColor" format="reference|color"/>
        <attr name="uidProgressBarCircularProgressColorList" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="UIDDialog">
        <attr name="uidDialogTitleTextColor" format="reference|color"/>
        <attr name="uidDialogBodyTextColor" format="reference|color"/>
        <attr name="uidDialogBackgroundColor" format="reference|color"/>
        <attr name="uidDialogShadowColor" format="reference|color"/>
        <attr name="uidDialogShadowAlpha" format="reference|float"/>
        <attr name="uidDialogDimSubtleLayerColor" format="reference|color"/>
        <attr name="uidDialogDimSubtleLayerAlpha" format="reference|float"/>
        <attr name="uidDialogDimStrongColor" format="reference|color"/>
        <attr name="uidDialogDimStrongColorAlpha" format="reference|float"/>
    </declare-styleable>

    <declare-styleable name="UIDLabel">
        <attr name="uidLabelTextColor" format="reference|color"/>
        <attr name="uidLabelTextAlpha" format="float"/>
        <attr name="uidLabelTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="UIDIndeterminateLinearProgressBar">
        <attr name="uidIndeterminateLinearProgressBGColor" format="reference|color"/>
        <attr name="uidIndeterminateLinearProgressBGAlpha" format="reference|float"/>
        <attr name="uidIndeterminateLinearProgressStartColor" format="reference|color"/>
        <attr name="uidIndeterminateLinearProgressCenterColor" format="reference|color"/>
        <attr name="uidIndeterminateLinearProgressEndColor" format="reference|color"/>
        <attr name="uidIndeterminateLinearProgressAnimDuration" format="reference|integer"/>
    </declare-styleable>

    <declare-styleable name="UIDProgressIndicatorButton">
        <attr name="uidIsIndeterminateProgressIndicator" format="reference|boolean"/>
        <attr name="uidProgressIndicatorButtonDrawable" format="reference"/>
        <attr name="uidProgressIndicatorButtonText" format="reference|string"/>
        <attr name="uidProgressIndicatorButtonProgress" format="reference|integer"/>
        <attr name="uidProgressIndicatorButtonProgressText" format="reference|string"/>
        <attr name="uidProgressIndicatorButtonProgressBackground" format="reference"/>
        <attr name="uidProgressIndicatorButtonProgressBackgroundColor" format="reference|color"/>
        <attr name="uidProgressIndicatorButtonProgressBackgroundColorAlpha" format="reference|float"/>
        <attr name="uidProgressIndicatorButtonProgressColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="UIDProgressIndicatorWithLabel">
        <attr name="uidIsIndeterminateProgressIndicator"/>
        <attr name="uidIsLinearProgressBar"/>
        <attr name="uidLabelTextAlpha"/>
        <attr name="android:text"/>
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="android:progress"/>
        <attr name="android:secondaryProgress"/>
        <attr name="uidLabelPosition" format="enum">
            <enum name="top_left" value="0"/>
            <enum name="top_right" value="1"/>
            <enum name="bottom_left" value="2"/>
            <enum name="bottom_right" value="3"/>
            <enum name="center" value="4"/>
            <enum name="bottom_center" value="5"/>
        </attr>
        <attr name="uidCircularProgressBarSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="middle" value="1"/>
            <enum name="big" value="2"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="UIDCheckBox">
        <attr name="uidCheckBoxPaddingStart" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="UIDSeparator"></declare-styleable>

    <declare-styleable name="UIDNavigationContainer">
        <attr name="uidShadowDrawable" format="reference"/>
        <attr name="uidShadowOffset" format="dimension|integer"/>
        <attr name="uidShadowType" format="enum">
            <enum name="primary" value="0"/>
            <enum name="secondary" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="UIDRecyclerView">
        <attr name="uidListPressedColor" format="reference|color"/>
        <attr name="uidListPressedAlpha" format="float"/>
    </declare-styleable>

    <declare-styleable name="UIDGradientView">
        <attr name="gradientStartColor" format="reference|color"/>
        <attr name="gradientEndColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="UIDInputValidation">
        <attr name="uidTextBoxValidationErrorDrawable" format="reference"/>
        <attr name="uidTextBoxValidationErrorText" format="reference|string"/>
    </declare-styleable>
</resources>
