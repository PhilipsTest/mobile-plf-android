<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Philipsuid">
        <attr name="uidIndeterminateLinearPBStyle" format="reference"/>
        <attr name="uidButtonDisabledAlpha" format="reference|float"/>
        <attr name="uidProgressIndicatorButtonBackgroundColor" format="reference|color"/>
        <attr name="uidProgressIndicatorButtonBackgroundAlpha" format="reference|float"/>
    </declare-styleable>

    <declare-styleable name="UIDComponentType">
        <attr name="uidComponentType" format="enum">
            <enum name="Primary" value="0"/>
            <enum name="AltPrimary" value="1"/>
            <enum name="Secondary" value="2"/>
            <enum name="Accent" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="UIDDotNavigation">
        <attr name="uidDotNavigationDrawable" format="reference"/>
        <attr name="uidDotNavigationIconSpacingLeft" format="reference|dimension"/>
        <attr name="uidDotNavigationIconSpacingRight" format="reference|dimension"/>
        <attr name="uidDotNavigationIconColorList" format="reference|color"/>
    </declare-styleable>

    <attr name="dotNavigationStyle"/>
</resources>


