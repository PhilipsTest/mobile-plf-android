<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="UIDCheckBox" parent="Base.Widget.AppCompat.CompoundButton.CheckBox">
        <item name="fontPath">fonts/centralesansbook.ttf</item>
        <item name="android:minHeight">@dimen/uid_checkbox_height</item>
        <item name="android:paddingEnd">@dimen/uid_checkbox_margin_left_right</item>
        <item name="android:paddingStart">@dimen/uid_checkbox_margin_left_right</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/uid_checkbox_text_size</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:background">@drawable/uid_checkbox_background</item>
        <item name="android:lineSpacingExtra">@dimen/uid_checkbox_line_spacing</item>
        <item name="android:lineSpacingMultiplier">@integer/uid_checkbox_font_spacing_multiplier</item>
    </style>
</resources>