<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:id="@+id/action_bar_lyt"
    android:layout_width="match_parent"
    android:layout_height="@dimen/actionbar_height"
    android:background="@drawable/actionbar_shape_gradient_bg" >

    <ImageView
        android:id="@+id/home_icon"
        style="@style/actionbarIconHomeImageView"
        android:contentDescription="@string/img_description" />

    <ImageView
        android:id="@+id/back_to_home_img"
        style="@style/actionbarIconArrowImageView"
        android:contentDescription="@string/img_description"
        />

    <com.philips.cl.di.dev.pa.digitalcare.customview.FontTextView
        android:id="@+id/action_bar_title"
        style="@style/actionbarFontTextView"
        custom:fontAssetName="fonts/centralesans-thin.otf" />

</RelativeLayout>