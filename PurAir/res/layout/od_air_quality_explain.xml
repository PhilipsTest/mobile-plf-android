                android:id="@+id/aqiAnalysisView8"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:layout_marginRight="4dp"
                android:background="#D61F2A" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <com.philips.cl.di.dev.pa.view.FontTextView android:lineSpacingExtra="5dp"
                    xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
                    android:id="@+id/aqiAnalysisMsg80"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="@string/outdoor_aiq_analysis_title1"
                    android:textColor="@color/gray"
                    android:textSize="18sp"
                    custom:fontAssetName="fonts/gillsanslight.ttf" />

                <com.philips.cl.di.dev.pa.view.FontTextView android:lineSpacingExtra="5dp"
                    xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
