                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="@string/outdoor_aiq_analysis_title6"
                    android:textColor="@color/gray"
                    android:textSize="18sp"
                    custom:fontAssetName="fonts/gillsanslight.ttf" />

                <com.philips.cl.di.dev.pa.view.FontTextView android:lineSpacingExtra="5dp"
                    xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
                    android:id="@+id/aqiAnalysisMsg131"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:paddingTop="4dp"
                    android:text="@string/outdoor_good"
                    android:textColor="@color/gray"
                    android:textSize="16sp"
                    custom:fontAssetName="fonts/gillsanslight.ttf" />
