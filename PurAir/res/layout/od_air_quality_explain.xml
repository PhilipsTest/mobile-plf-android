                    android:paddingTop="4dp"
                    android:text="@string/outdoor_slightly_polluted"
                    android:textColor="@color/gray"
                    android:textSize="16sp"
                    custom:fontAssetName="fonts/gillsanslight.ttf" />
            </LinearLayout>
        </LinearLayout>

