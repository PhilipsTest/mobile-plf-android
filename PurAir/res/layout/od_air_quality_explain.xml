<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:padding="15dp" >

        <ImageView
            android:id="@+id/aqiAnalysisClose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:contentDescription="@string/img_description"
            android:padding="8dp"
            android:src="@drawable/help_close" />

        <com.philips.cl.di.dev.pa.view.FontTextView android:lineSpacingExtra="5dp"
            xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
            android:id="@+id/aqiAnalysisTitle5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="8dp"
            android:layout_toLeftOf="@+id/aqiAnalysisClose"
            android:text="@string/outdoor_aiq_analysis_head5"
            android:textColor="@color/cornflowerblue"
            android:textSize="24sp"
            custom:fontAssetName="fonts/gillsanslight.ttf" />

