                    android:id="@+id/aqiAnalysisMsg61"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:paddingTop="4dp"
                    android:text="@string/indoor_moderate"
                    android:textColor="@color/gray"
                    android:textSize="16sp"
                    custom:fontAssetName="fonts/gillsans.ttf" />
            </LinearLayout>
        </LinearLayout>
        <View
            android:id="@+id/odView32"
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:layout_below="@+id/aqiAnalysisLayout6"
		    android:background="@color/gray" />
       <LinearLayout android:id="@+id/aqiAnalysisLayout7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/aqiAnalysisLayout6"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp">
            
