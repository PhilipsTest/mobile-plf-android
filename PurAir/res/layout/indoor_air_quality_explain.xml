            android:layout_marginTop="12dp"
            android:text="@string/outdoor_aiq_analysis_head3"
            android:textColor="@color/dodgerblue"
            android:textSize="16sp"
            custom:fontAssetName="fonts/gillsans.ttf" />
        
        <com.philips.cl.di.dev.pa.view.FontTextView
            xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
            android:id="@+id/aqiAnalysisMsg30"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/aqiAnalysisTitle3"
            android:layout_marginTop="6dp"
            android:text="@string/outdoor_analysis_detail_head3"
            android:textColor="@color/gray"
            android:textSize="16sp"
            custom:fontAssetName="fonts/gillsans.ttf" />
        
