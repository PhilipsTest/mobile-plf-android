                    android:layout_marginLeft="4dp"
                    android:text="@string/indoor_aiq_analysis_title4"
                    android:textColor="@color/gray"
                    android:textSize="18sp"
                    custom:fontAssetName="fonts/gillsans.ttf" />

                <com.philips.cl.di.dev.pa.view.FontTextView
                    xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
                    android:id="@+id/aqiAnalysisMsg71"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:paddingTop="4dp"
                    android:text="@string/indoor_good"
                    android:textColor="@color/gray"
                    android:textSize="16sp"
                    custom:fontAssetName="fonts/gillsans.ttf" />
            </LinearLayout>
        </LinearLayout>
       
        
