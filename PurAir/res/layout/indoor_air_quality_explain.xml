<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:padding="16dp" >

        <ImageView
            android:id="@+id/aqiAnalysisClose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:paddingBottom="8dp"
            android:contentDescription="@string/img_description"
            android:src="@drawable/help_close" />

        <com.philips.cl.di.dev.pa.view.FontTextView
            xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
            android:id="@+id/aqiAnalysisHeading"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/aqiAnalysisClose"
            android:text="@string/outdoor_aiq_analysis_head1"
            android:textColor="@color/dodgerblue"
            android:textSize="24sp"
            custom:fontAssetName="fonts/gillsans.ttf" />

        <com.philips.cl.di.dev.pa.view.FontTextView
            xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
            android:id="@+id/aqiAnalysisMsg10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/aqiAnalysisHeading"
            android:layout_marginTop="6dp"
            android:text="@string/outdoor_analysis_detail1_head1"
            android:textColor="@color/gray"
            android:textSize="16sp"
            custom:fontAssetName="fonts/gillsans.ttf" />

        <com.philips.cl.di.dev.pa.view.FontTextView
            xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
            android:id="@+id/aqiAnalysisMsg11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/aqiAnalysisMsg10"
            android:layout_marginTop="6dp"
            android:text="@string/outdoor_analysis_detail2_head100"
            android:textColor="@color/gray"
            android:textSize="16sp"
            custom:fontAssetName="fonts/gillsans.ttf" />

        <com.philips.cl.di.dev.pa.view.FontTextView
            xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
            android:id="@+id/aqiAnalysisTitle2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/aqiAnalysisMsg11"
            android:layout_marginTop="12dp"
            android:text="@string/outdoor_aiq_analysis_head2"
            android:textColor="@color/dodgerblue"
            android:textSize="16sp"
            custom:fontAssetName="fonts/gillsans.ttf" />

        <com.philips.cl.di.dev.pa.view.FontTextView
            xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
            android:id="@+id/aqiAnalysisMsg20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/aqiAnalysisTitle2"
            android:layout_marginTop="6dp"
            android:text="@string/outdoor_analysis_detail_head2"
            android:textColor="@color/gray"
            android:textSize="16sp"
            custom:fontAssetName="fonts/gillsans.ttf" />

