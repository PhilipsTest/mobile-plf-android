            android:layout_marginTop="12dp"
            android:text="@string/outdoor_aiq_analysis_head4"
            android:textColor="@color/dodgerblue"
            android:textSize="16sp"
            custom:fontAssetName="fonts/gillsans.ttf" />

        <LinearLayout android:id="@+id/aqiAnalysisLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/aqiAnalysisTitle4"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp">
            
            <View
                android:id="@+id/aqiAnalysisView4"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:layout_marginRight="4dp"
                android:background="#D61F2A" /> 

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" 
                >

                <com.philips.cl.di.dev.pa.view.FontTextView
                    xmlns:custom="http://schemas.android.com/apk/res/com.philips.cl.di.dev.pa"
                    android:id="@+id/aqiAnalysisMsg40"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="@string/indoor_aiq_analysis_title1"
                    android:textColor="@color/gray"
                    android:textSize="18sp"
                    custom:fontAssetName="fonts/gillsans.ttf" />

