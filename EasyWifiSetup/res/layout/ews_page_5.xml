<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout4"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/easy_wifi_setup_5"
    android:orientation="vertical"
    tools:context="com.philips.cl.ews.EwsPageFragment" >

    <ImageButton
        android:id="@+id/back4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="@color/transparent"
        android:onClick="goBack"
        android:src="@drawable/back_button" 
        android:hint="previous"/>


    <RelativeLayout
        android:id="@+id/viewGifFinish"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="130dp" >

        <ImageView
            android:id="@+id/blue_led"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:src="@drawable/blinking_blue" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textViewFinishScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/back4"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="34dp"
        android:text="@string/finish_screen_info"
        android:gravity="center"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout>